#include <iostream>
using namespace std;

class Animal {
public:
  void eat() {
    std::cout << "Animal is eating\n";
  }
};

class Dog : public Animal {
public:
  // Метод eat() не перекрывается
};

int main() {
  Dog myDog;
  myDog.eat(); // Вывод: Animal is eating (Вызывается унаследованный метод из Animal)
  return 0;
}
